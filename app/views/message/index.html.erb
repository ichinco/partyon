<%= render partial:"layouts/trip_nav", locals:{trip: @trip} %>

<div class="container">
    <h1>Group Messages</h1>

    <div>
        <% @messages.each do |message| %>
            <div>
                <%= link_to message.subject, trip_message_path(@trip, message) %>
            </div>
        <% end %>
    </div>
</div>